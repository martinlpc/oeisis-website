{
  "extends": "astro/tsconfigs/base",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "strict": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "target": "ESNext",
    "lib": ["ESNext", "DOM"],
    "types": ["astro/astro-jsx", "astro/client"]
  }
}
